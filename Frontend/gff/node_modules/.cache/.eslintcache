ight_int = parseFloat(user['height'],10);\n        var bmi = weight_int/(height_int*height_int);\n        console.log(weight_int);\n        console.log(height_int);\n        console.log(bmi);\n        this.setState({bmi: String(bmi)})\n    }\n\n\n    render() {\n    const {user, isAuthenticated, LogIn, LogOut} = this.context;\n    return (\n        <div className='sportsman-profile'>\n            <div className='sportsman-profile-container'>\n                <h1>{user['firstName']} {user['lastName']}</h1>\n                <br/>\n                <br/>\n                <div>User Name: {user['userName']}</div>\n                <br/>\n                <div>Email: {user['email']}</div>\n                <br/>\n                <div>Born In: {user['dayOfBirth']}/{user['monthOfBirth']}/{user['yearOfBirth']}</div>\n                <br/>\n                <div>Height: {user['height']}</div>\n                <br/>\n                <div>Weight: {user['weight']}</div>\n                <br/>\n                <div>\n                <button type='submit' onClick={() => this.bmi()}>Calc BMI:</button>\n                <label>{this.state.bmi}</label>\n                </div>\n                <br/>\n                <div>Phone Number: {user['phoneNumber']}</div>\n                <br/>\n                <div>Sport: {user['sport']}</div>\n                <br/>\n                <div>Level: {user['level']}</div>\n            </div>\n        </div> \n    )\n    }\n}\n\nSportsmanProfile.contextType = UserContext;\n\nexport default SportsmanProfile;",{"ruleId":"96","severity":1,"message":"97","line":8,"column":33,"nodeType":"98","messageId":"99","endLine":8,"endColumn":38},{"ruleId":"96","severity":1,"message":"97","line":6,"column":33,"nodeType":"98","messageId":"99","endLine":6,"endColumn":38},{"ruleId":"96","severity":1,"message":"100","line":6,"column":40,"nodeType":"98","messageId":"99","endLine":6,"endColumn":46},{"ruleId":"96","severity":1,"message":"101","line":1,"column":17,"nodeType":"98"